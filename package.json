{
  "name": "litium-headless-accelerator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --check .",
    "format": "prettier --write .",
    "test": "jest --watch",
    "test:ci": "jest --coverage --ci",
    "test:e2e": "playwright test",
    "proxy": "litium-storefront proxy --litium $(grep RUNTIME_LITIUM_SERVER_URL .env.local | cut -d '=' -f2) --storefront http://localhost:3000",
    "generate-possible-types": "node scripts/generate-possible-types.js",
    "generate-block-components": "node scripts/generate-possible-types.js && node scripts/generate-block-components.js",
    "predev": "yarn generate-possible-types",
    "prepare": "husky"
  },
  "dependencies": {
    "@apollo/client": "3.11.8",
    "@apollo/experimental-nextjs-app-support": "0.11.3",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.0",
    "@hookform/resolvers": "3.3.4",
    "@mui/material": "5.15.13",
    "@mui/x-tree-view": "6.17.0",
    "@react-email/components": "0.0.15",
    "@react-email/render": "0.0.12",
    "@types/nodemailer": "6.4.14",
    "clsx": "2.1.0",
    "globalize": "^0.1.1",
    "graphql": "16.8.1",
    "next": "14.2.7",
    "nodemailer": "6.9.12",
    "rc-slider": "10.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.51.0",
    "react-responsive-carousel": "3.2.23",
    "sass": "1.71.1",
    "server-only": "0.0.1",
    "set-cookie-parser": "2.6.0",
    "sharp": "^0.33.5",
    "swiper": "11.0.7",
    "use-debounce": "10.0.0",
    "use-onclickoutside": "0.4.1",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@playwright/test": "1.42.1",
    "@testing-library/dom": "9.3.4",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.27",
    "@types/react": "18.2.65",
    "@types/react-dom": "18.2.22",
    "@types/set-cookie-parser": "2.4.7",
    "autoprefixer": "10.4.18",
    "cross-fetch": "4.0.0",
    "dotenv-flow": "4.1.0",
    "eslint": "8.57.0",
    "eslint-config-next": "^14.2.7",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.2.2",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.12",
    "tailwindcss": "3.4.1",
    "typescript": "5.4.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md}": "prettier --write"
  },
  "engines": {
    "node": "20"
  },
  "resolutions": {
    "string-width": "4.2.3",
    "wrap-ansi": "7.0.0"
  }
}
